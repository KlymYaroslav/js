show databases;
use yaroslav;
show tables;
select * from cars;
select * from cars where year>2000;
select * from cars where year<2015;
select * from cars where year between 2008 and 2010;
select * from cars where year!=2008 or year!=2009 or year!=2010;
select * from cars where year=price;
select * from cars where model='bmw' and year>2014;
select * from cars where model='audi' and year<2014;
select * from cars limit 5;
select * from cars limit 5 offset 212;
select avg(price) from cars where model='KIA';
select avg(price) from cars;
select sum(id) from cars;
select count(model) from cars;
